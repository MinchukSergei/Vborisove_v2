<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <script src="../../js/jquery-2.1.4.js"></script>
  <script src="../../js/bootstrap.min.js"></script>
  <link rel='stylesheet' href='../../css/bootstrap.min.css' type='text/css' media='all'>
  <link rel='stylesheet' href="../../css/navigation.css">
  <link rel='stylesheet' href="../../css/registration.css">
</head>
<body>

<nav class="navbar navbar-fixed-top navbar-default">
  <div class="container">
    <div class="navbar-header">
      <img alt="Brand" src="../../img/vb.png">
    </div>
  </div>
</nav>


<div class="col-md-4 col-md-offset-4 col-xs-6 col-xs-offset-1">
  <div class="container-fluid">
    <div>
      <legend class="">Register</legend>
    </div>

    <div class="control-group" id="first-control">
      <!-- Name -->
      <div class="controls">
        <label>Name</label>
        <input type="text" placeholder="Enter you name" class="form-control">
      </div>
    </div>

    <div class="control-group">
      <!-- Username -->
      <div class="controls">
        <label>Username</label>
        <input type="text" placeholder="Enter username" class="form-control">
      </div>
    </div>



    <div class="control-group">
      <!-- E-mail -->
      <div class="controls">
        <label>E-mail</label>
        <input type="text" placeholder="Please provide your E-mail" class="form-control">
      </div>
    </div>

    <div class="control-group">
      <!-- Password-->
      <div class="controls">
          <label>Password</label>
          <input type="text" placeholder="Minimum 4 characters" class="form-control">
      </div>
    </div>

    <div class="control-group">
      <!-- Confirm -->
      <div class="controls">
        <label>Confirm</label>
        <input type="text" placeholder="Please confirm password" class="form-control">
      </div>
    </div>

    <div class="control-group">
      <!-- Button -->
      <div class="col-md-4 col-md-offset-4 col-xs-6 col-xs-offset-3">
        <div class="controls">
          <button class="btn btn-primary btn-block">Register</button>
        </div>
      </div>
    </div>
  </div>
</div>

<footer class="footer">
  <div class="container">
      <p class="text-muted">Copyright Â© 2015 Minchuk Sergei.</p>
    </div>
  </div>
</footer>

</body>
</html>

